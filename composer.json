{
    "name": "theaddresstechnology/ddd",
    "description": "DDD Laravel-structure-generator",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "theaddresstechnology",
            "email": "islamadel295@yahoo.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-query-builder": "^5.7",
        "the42coders/workflows": "^0.0.19"
    },
    "autoload": {
        "psr-4": {
            "theaddresstechnology\\DDD\\":"src/",
            "App\\":"app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "theaddresstechnology\\DDD\\":"src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel":{
            "providers":[
                "theaddresstechnology\\DDD\\DomainDriverDesignServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.42"
    }
}
